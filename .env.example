# Solana RPC URL - Use a reliable Solana RPC provider
RPC_URL=https://api.mainnet-beta.solana.com

# Private key of your Solana wallet (base58 encoded)
PRIVATE_KEY=your_private_key_here

# Token mint address of your pump.fun token
TOKEN_MINT=YourTokenMintAddressHere

# Pump.fun program ID (check their docs for the current address)
PUMP_PROGRAM_ID=YourPumpProgramIdHere

# Minimum SOL amount to trigger a bundle (in SOL)
MIN_SOL_AMOUNT=0.1

# Slippage tolerance in %
SLIPPAGE=1.0

# Optional: Telegram bot token and chat ID for notifications
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token
# TELEGRAM_CHAT_ID=your_telegram_chat_id
